---
import layout from '~/layouts/Wiki/CountryShowLayout.astro'
import { SITE } from "~/config.mjs";
export async function getStaticPaths() {
  const data = await fetch(`${SITE.apiUrl}/countries.json`).then((response) =>
    response.json()
  );

  return data.map((country) => {
    return {
      params: { id: country.id },
    };
  });
}
