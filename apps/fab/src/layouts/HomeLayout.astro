---
import Layout from "~/layouts/PageLayout.astro";
import FabFrontPage from "./Pages/FabFrontPage.astro";
import CSFrontPage from "./Pages/CSFrontPage.astro";
const scope = import.meta.env.COUNTRY_SCOPE
const {content} = Astro.props;
---

<Layout>

{scope === 'all' ? <FabFrontPage content={content} /> : <CSFrontPage />}

</Layout>
